open "text.jpg" as target
apply erode 5 to target
apply dilate 3 to target
save target as "text-output.jpg"
open "man.jpg" as man
apply erode 3 to man
apply dilate 3 to man
save man as "man-output.jpg"
open "pinocchio.jpg" as pic
apply outline 5 to pic
save pic as "pinocchio-outline.jpg"
open "pinocchio.jpg" as pic2
apply grayscale to pic2
apply outline 20 to pic2
apply erode 8 to pic2
save pic2 as "pinocchio-bw-outline.jpg"
